@model SearchResultVM

@{
    string isLast = ((bool)ViewData[GlobalStatic.Islast]).ToString().ToLower();
}

@for (int i = 0; i < Model.Products.Count(); i++)
{
    @Html.Partial("_ProductItem", Model.Products[i])
}

<script type="text/javascript">
    $(document).ready(function () {
        if ('@isLast' == 'true')
            $("#btnLoadMore").hide();
    })
</script>